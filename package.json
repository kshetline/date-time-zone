{
  "name": "ks-date-time-zone",
  "version": "1.6.1",
  "description": "Date/time, IANA time zones, calendar with settable Julian/Gregorian switchover",
  "main": "dist/index.js",
  "typings": "dist/index",
  "scripts": {
    "build": "tsc",
    "test": "karma start",
    "prepublishOnly": "tsc",
    "watch:build": "tsc --watch",
    "document": "typedoc --name \"ks-date-time-zone\" --module commonjs --exclude \"**/*+(e2e|spec|index).ts\" --excludePrivate --excludeProtected --readme README.md --target ES5 --out docs src"
  },
  "keywords": [
    "calendar",
    "date",
    "gregorian",
    "iana",
    "julian",
    "time",
    "timezone",
    "zone"
  ],
  "author": "Kerry Shetline <kerry@shetline.com>",
  "license": "MIT",
  "dependencies": {
    "ks-math": "^1.1.6",
    "ks-util": "^1.4.3",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.16",
    "@types/lodash": "^4.14.123",
    "jasmine-core": "^2.99.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-typescript": "^4.0.0",
    "karma-typescript-es6-transform": "^4.0.0",
    "tslint": "^5.15.0",
    "typescript": "^2.9.2"
  },
  "repository": "github:kshetline/ks-date-time-zone"
}
