{
  "name": "@tubular/time",
  "version": "2.1.0",
  "description": "Date/time, IANA time zones, calendar with settable Julian/Gregorian switchover",
  "browser": "dist/index.umd.js",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "esm2015": "dist/index.js",
  "typings": "dist/index",
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist/ && tsc && webpack && webpack --env target=umd && ts-node make-remote-zone-data.ts",
    "prepublishOnly": "tsc",
    "lint": "eslint 'src/**/*.ts' '**/*.cjs'",
    "test": "nyc --reporter=html mocha --require ts-node/register src/**/*.spec.ts",
    "document": "typedoc --name \"ks-date-time-zone\" --module commonjs --exclude \"**/*+(e2e|spec|index).ts\" --excludePrivate --excludeProtected --readme README.md --target ES5 --out docs src"
  },
  "keywords": [
    "calendar",
    "date",
    "gregorian",
    "iana",
    "julian",
    "time",
    "timezone",
    "zone"
  ],
  "author": "Kerry Shetline <kerry@shetline.com>",
  "license": "MIT",
  "dependencies": {
    "@tubular/math": "^2.0.5",
    "@tubular/util": "^3.0.4",
    "by-request": "^1.1.6",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "@types/chai": "^4.2.14",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "esm": "^3.2.25",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  },
  "repository": "github:kshetline/tubular_time.git"
}
